{
	"name": "PricesheetExportSource",
	"properties": {
		"linkedServiceName": {
			"referenceName": "s037-cost-management-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fromDate": {
				"type": "string"
			},
			"toDate": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "*.csv",
				"folderPath": {
					"value": "@concat('pricesheet/portal-export/FinOps-Pricesheet-Monthly/', dataset().fromDate, '-', dataset().toDate, '/*')",
					"type": "Expression"
				},
				"fileSystem": "usage"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "EnrollmentNumber",
				"type": "String"
			},
			{
				"name": "MeterID",
				"type": "String"
			},
			{
				"name": "MeterName",
				"type": "String"
			},
			{
				"name": "MeterType",
				"type": "String"
			},
			{
				"name": "MeterCategory",
				"type": "String"
			},
			{
				"name": "MeterSubCategory",
				"type": "String"
			},
			{
				"name": "ServiceFamily",
				"type": "String"
			},
			{
				"name": "Product",
				"type": "String"
			},
			{
				"name": "SkuID",
				"type": "String"
			},
			{
				"name": "ProductID",
				"type": "String"
			},
			{
				"name": "MeterRegion",
				"type": "String"
			},
			{
				"name": "UnitOfMeasure",
				"type": "String"
			},
			{
				"name": "PartNumber",
				"type": "String"
			},
			{
				"name": "EffectiveStartDate",
				"type": "String"
			},
			{
				"name": "EffectiveEndDate",
				"type": "String"
			},
			{
