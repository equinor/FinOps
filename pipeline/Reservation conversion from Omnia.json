{
	"name": "Reservation conversion from Omnia",
	"properties": {
		"activities": [
			{
				"name": "Convert json to parquet",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "tempdata/monthly/2022/*",
							"wildcardFileName": "costreport_reservations_monthly_*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ACDC_reservation_data_json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "S037_reservation_data_parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "PipelinesNotInUse"
		},
		"annotations": [],
		"lastPublishTime": "2022-08-23T15:06:04Z"
	},
	"type": "Microsoft.Synapse/workspaces/pipelines"
}