{
	"name": "convert-pricesheet-to-parquet",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "pricesheet/portal-export/FinOps-Pricesheet-Monthly/20240101-20240131/*",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "PricesheetExportSource",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "PipelinesInProduction/Pricesheet"
		},
		"annotations": []
	}
}