				"source": [
					"# Aggregate the data \r\n",
					"Reduce the size of the data to fit PowerBI performance\r\n",
					"\r\n",
					"Only select the useful fields"
				]
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
