				"source": [
					"service_df = service_df.withColumnRenamed('Service Model', 'ServiceModel')\r\n",
					"service_df = service_df.select('service_join_key', 'ServiceModel')\r\n",
					"service_df = service_df.dropDuplicates([\"service_join_key\", \"ServiceModel\"])"
