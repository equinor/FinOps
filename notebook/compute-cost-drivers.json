				},
				"source": [
					"cost_df = cost_df \\\r\n",
					"    .select('ShortDate', 'MeterCategory', 'MeterSubCategory', 'CostInBillingCurrency') \\\r\n",
					"    .groupBy('ShortDate', 'MeterCategory', 'MeterSubCategory') \\\r\n",
					"    .agg(F.sum('CostInBillingCurrency').alias('Cost'))"
				],
				"execution_count": 16
			},
