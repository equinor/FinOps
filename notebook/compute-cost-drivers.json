						}
					}
				},
				"source": [
					"cost_df = cost_df.withColumn('ShortDate', F.date_format(F.col('Date'), 'yyyyMM'))\r\n",
					"cost_df = cost_df.where(F.col('ShortDate').isin([start_date, end_date]))"
				],
				"execution_count": 15
			},
			{
				"cell_type": "code",
