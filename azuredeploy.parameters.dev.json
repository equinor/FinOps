{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "synapseWorkspaceName": {
            "value": "finops-synapse-dev"
        },
        "storageAccountName": {
            "value" : "finopssadev"
        },
        "storageAccountType": {
            "value": "Standard_RAGRS"
        },
        "sparkPoolName": {
            "value": "sparkpooldev"
        },
        "nodeSize": {
            "value": "Medium"
        },
        "sparkPoolMinNodeCount": {
            "value": 3
        },
        "sparkPoolMaxNodeCount": {
            "value": 5
        },
        "sparkPoolDelayInMinutes": {
            "value": 10
        },
        "sparkVersion": {
            "value": "3.3"
        }
    }
}